{
  "name": "@treeunfe/treeunfe-dfe",
  "repository": {
    "type": "git",
    "url": "https://github.com/treeunfe/treeunfe-dfe"
  },
  "publishConfig": {
    "@treeunfe:registry": "https://npm.pkg.github.com"
  },
  "version": "0.4.9",
  "description": "TreeunfeDFe é uma biblioteca Node.js projetada para simplificar a interação com os webservices da SEFAZ, proporcionando uma solução robusta para automação de processos relacionados à Nota Fiscal Eletrônica (NF-e).",
  "type": "module",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.cjs",
    "import": "./dist/esm/index.js"
  },
  "author": "Junior Trojilio",
  "scripts": {
    "dev": "tsx src/testes.ts",
    "debug": "tsx src/debug.ts",
    "test": "jest --passWithNoTests",
    "clean": "rm -rf dist",
    "build": "node build.mjs",
    "build:rp": "npm run clean && rollup -c",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs-esm": "yarn build:cjs && yarn build:esm",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:first": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release:prepare": "npm run build && npm test",
    "release:patch": "npm run version:patch && npm run changelog && npm run release:prepare",
    "release:minor": "npm run version:minor && npm run changelog && npm run release:prepare",
    "release:major": "npm run version:major && npm run changelog && npm run release:prepare",
    "commit": "cz",
    "prepare": "husky install"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "nfe",
    "nfce",
    "nota fiscal eletrônica",
    "dfe",
    "sefaz",
    "danfe",
    "recepção de eventos",
    "fiscal",
    "eventos nfe",
    "epec"
  ],
  "dependencies": {
    "@types/ini": "^4.1.0",
    "@types/node-forge": "^1.3.11",
    "@types/pako": "^2.0.3",
    "@types/pdfmake": "^0.2.9",
    "@types/pem": "^1.14.4",
    "@types/xml2js": "^0.4.14",
    "@types/xsd-schema-validator": "^0.5.7",
    "axios": "^1.7.7",
    "bwip-js": "^4.3.2",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^17.2.1",
    "easy-soap-request": "^5.4.0",
    "ini": "^4.1.1",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.2",
    "libxmljs2": "^0.37.0",
    "node-fetch": "^3.3.2",
    "node-forge": "^1.3.1",
    "nodemailer": "^6.9.14",
    "pako": "^2.1.0",
    "pdfkit": "^0.15.0",
    "pdfmake": "^0.2.10",
    "pem": "^1.14.8",
    "qrcode": "^1.5.4",
    "rollup-plugin-node-builtins": "^2.0.0",
    "sha1": "^1.1.1",
    "soap": "^1.0.0",
    "tsc-alias": "^1.8.10",
    "winston": "^3.17.0",
    "xml-crypto": "^6.0.0",
    "xml-js": "^1.6.11",
    "xml2js": "^0.6.2",
    "xsd-assembler": "^0.0.3",
    "xsd-schema-validator": "^0.7.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.7",
    "@types/nodemailer": "^6.4.15",
    "@types/qrcode": "^1.5.5",
    "@types/sha1": "^1.1.5",
    "conventional-changelog-cli": "^4.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "semantic-release": "^22.0.5",
    "commitizen": "^4.3.0",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "husky": "^8.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^29.7.0",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-esbuild-minify": "^1.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.4",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5"
  }
}
